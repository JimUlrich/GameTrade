@model GameTrade.ViewModels.LookupByTitleViewModel



<div class="form-group">
    <label asp-for="GameId" class="col-lg-2"></label>
    <div class="col-lg-10">
        <select asp-for="GameId" asp-items="@Model.Games" class="form-control"></select>
    </div>
</div>

<div class="form-group">
    <div class="col-lg-10 col-lg-offset-2">
        <input type="submit" class="btn btn-success" value="Select Game" asp-action="LookupByTitle" />
    </div>
</div>

