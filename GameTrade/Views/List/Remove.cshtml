@model List<GameTrade.Models.Game>

<form method="post">

    @foreach (var game in Model)
    {
        <div>
        <input type="checkbox" name="GameIds" id="@game.GameId" value="@game.GameId" />
        <label for="@game.GameId">@game.Title</label>
        </div>
    }

    <input type="submit" value="Remove Selected Games" />

</form>