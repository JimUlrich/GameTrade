@model GameTrade.ViewModels.SearchByViewModel

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Platform</th>
            <th>Year</th>
            <th>GameCondition</th>
            <th>Value</th>
        </tr>
    </thead>
   
    <tbody>
   @{foreach (var game in Model.Games)
    {

        <tr>
            <td>@game.Title</td>
            <td>@game.System</td>
            <td>@game.Year</td>
            <td>@game.GameCondition</td>
            <td>$@game.Value</td>
        </tr>
     }}
    </tbody>
</table>
